<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.firebase.database">
  <!--<uses-sdk android:minSdkVersion="14" android:targetSdkVersion="23"/>-->
  <uses-permission android:name="android.permission.INTERNET"/>
  <application android:label="FirebaseDatabaseTests" android:largeHeap="true">
    <uses-library android:name="android.test.runner" />
    <service android:name="com.google.firebase.components.ComponentDiscoveryService"
        android:exported="false">
      <meta-data android:name="com.google.firebase.components:com.google.firebase.database.TestComponentRoboRegistrar"
          android:value="com.google.firebase.components.ComponentRegistrar" />
    </service>
  </application>

  <instrumentation
      android:name="com.google.android.apps.common.testing.testrunner.Google3InstrumentationTestRunner"
      android:targetPackage="com.google.firebase.database" />

</manifest>
